{
    "providers": {
        "namecheap": {
            "api_url": "https://dynamicdns.park-your-domain.com/update",
            "required_fields": ["domain", "password", "subdomains"]
        },
        "cloudflare": {
            "api_url": "https://api.cloudflare.com/client/v4/",
            "required_fields": ["zone_id", "api_token", "subdomains"]
        },
        "google": {
            "api_url": "https://domains.google.com/nic/update",
            "required_fields": ["domain", "password"]
        },
        "aws": {
            "api_url": "https://route53.amazonaws.com/2013-04-01/",
            "required_fields": ["access_key", "secret_key", "zone_id", "subdomains"]
        },
        "digitalocean": {
            "api_url": "https://api.digitalocean.com/v2/domains/",
            "required_fields": ["token", "domain", "subdomains"]
        },
        "linode": {
            "api_url": "https://api.linode.com/v4/domains/",
            "required_fields": ["token", "domain", "subdomains"]
        },
        "cloudns": {
            "api_url": "https://api.cloudns.net/dynamicURL/",
